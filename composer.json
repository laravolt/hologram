{
    "name": "laravolt/hologram",
    "description": "Laravel activity log viewer",
    "keywords": [
        "laravel",
        "activity log",
        "laravolt",
        "hologram"
    ],
    "homepage": "https://github.com/laravolt/hologram",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        },
        {
            "name": "Ramadani",
            "email": "email.ramadani@gmail.com",
            "homepage": "http://daniramadani.web.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.2",
        "laravolt/suitable": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*|5.*"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Hologram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Hologram\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
